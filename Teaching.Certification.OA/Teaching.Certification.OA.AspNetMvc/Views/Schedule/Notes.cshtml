@model IPageable<Note>
@{
    ViewData["Title"] = "我的便签";
}
<!-- My Notes -->
<div class="row">
    <div class="col-sm-6">

        <table class="table">
            <caption>便签列表</caption>
            <thead>
                <tr>
                    <th>编号</th>
                    <th>标题</th>
                    <th>描述</th>
                    <th>编辑</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var m in Model)
                {
                    <tr>
                        <td>@m.Id</td>
                        <td>@m.Title</td>
                        <td>@m.Descr</td>
                        <td>修改</td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>